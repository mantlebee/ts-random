{
  "name": "@mantlebee/ts-random",
  "version": "0.5.1",
  "description": "Utils function to generate random data.",
  "main": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "docs/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "build": "rollup --config && tsc-alias",
    "coverage": "jest --coverage --collectCoverageFrom='./src/**'",
    "doc": "typedoc",
    "doc:publish": "npm run doc && node ./gh-pages.js",
    "prepublishOnly": "npm run build && npm run doc",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mantlebee/ts-random.git"
  },
  "keywords": [
    "typescript",
    "common",
    "utilities"
  ],
  "author": "MantleBee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mantlebee/ts-random/issues"
  },
  "homepage": "https://github.com/mantlebee/ts-random#readme",
  "dependencies": {
    "@mantlebee/ts-core": "0.24.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "8.3.2",
    "@types/jest": "24.0.19",
    "gh-pages": "^6.0.0",
    "jest": "26.6.3",
    "rollup": "2.75.5",
    "rollup-plugin-cleaner": "1.0.0",
    "ts-jest": "26.4.3",
    "tsc-alias": "1.6.8",
    "typedoc": "0.22.17",
    "typescript": "4.7.2"
  }
